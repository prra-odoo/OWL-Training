<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
	<t t-name="owl_shop.product" owl="1">
		<div class="p-3">
	        <t t-esc="this.props.id"/>
	        Product Name:<t t-esc="this.props.product_name"/>
	        Price:<t t-esc="this.props.product_price"/>
	        <button type="button" t-on-click="addProduct">ADD TO CART</button>
		</div>
    </t>
</templates>