<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="owl_playground.card" owl="1">
        <div class="card" style="width: 18rem;">
            <t t-if="props.slots.title"> 
                <h5 class="card-title"><t t-slot="title"/></h5>
            </t>
            <div class="card-body">
                <t t-slot="content"/>
           	</div>
            <t t-slot="default"/>
        </div>
    </t>
</templates>